    "react-dom": "17.0.2",
    "react-headroom": "^3.1.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-headroom": "^2.2.1",
