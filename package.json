    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "remark": "^13.0.0",
    "strip-markdown": "^4.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-lostfictions": "^0.11.0",
